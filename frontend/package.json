    "name": "details",
    "description": "DeTAILS - Deductive Thematic Analysis with Iterative LLM Support",
    "version": "1.0.0",
    "devDependencies": {
        "@electron-forge/cli": "^7.6.1",
        "@electron-forge/maker-deb": "^7.6.1",
        "@electron-forge/maker-dmg": "^7.6.1",
        "@electron-forge/maker-rpm": "^7.6.1",
        "@electron-forge/maker-squirrel": "^7.6.1",
        "@electron-forge/maker-zip": "^7.6.1",
        "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
        "@electron-forge/plugin-fuses": "^7.6.1",
        "@electron/fuses": "^1.8.0",
        "@types/d3": "^7.4.3",
        "@types/node": "^22.9.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/react-router-dom": "^5.3.3",
        "autoprefixer": "^10.4.13",
        "concurrently": "^9.1.0",
        "cross-env": "^7.0.3",
        "electron": "^33.2.0",
        "mammoth": "^1.8.0",
        "postcss": "^8.4.19",
        "prettier": "^3.3.3",
        "react-scripts": "^5.0.1",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.15",
        "taze": "^0.18.0",
        "typescript": "^4.9.5",
        "wait-on": "^8.0.1"
    },
    "type": "commonjs",
    "dependencies": {
        "@electron/remote": "^2.0.8",
        "@getstation/electron-google-oauth2": "^14.0.0",
        "@langchain/community": "^0.3.15",
        "@langchain/ollama": "^0.1.2",
        "auto-launch": "^5.0.5",
        "chromadb": "^1.9.2",
        "chromadb-default-embed": "^2.13.2",
        "chrome-launcher": "^1.1.2",
        "core-js": "^3.39.0",
        "d3": "^7.9.0",
        "electron-is-dev": "^3.0.1",
        "electron-log": "^5.3.0",
        "electron-reloader": "^1.2.3",
        "electron-squirrel-startup": "^1.0.1",
        "electron-updater": "^6.3.9",
        "framer-motion": "^11.14.1",
        "fuzzball": "^2.1.3",
        "langchain": "^0.3.6",
        "pdf-parse": "^1.1.1",
        "pdfjs-dist": "^4.10.38",
        "puppeteer": "^23.8.0",
        "puppeteer-core": "^23.8.0",
        "react": "^18.2.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.2.0",
        "react-icons": "^5.4.0",
        "react-router-dom": "^6.4.4",
        "react-toastify": "^10.0.6",
        "react-use": "^17.4.0",
        "sqlite3": "^5.1.7",
        "tailwind-scrollbar": "^3.1.0",
        "web-streams-polyfill": "^4.0.0",
        "xlsx": "^0.18.5"
    },
    "homepage": "./",
    "main": "electron",
    "scripts": {
        "dev": "concurrently \"npm run react:dev\" \"npm run electron:wait\"",
        "executables": "cd ../executables && rm -f ./test.db && rm -rf ./chroma_data && concurrently \"./chroma/cli run\" \"./ollama/ollama-darwin serve\"",
        "run:chroma": "./chroma/cli run",
        "run:ollama": "./ollama/ollama-darwin serve",
        "electron:wait": "wait-on tcp:3000 && npm run electron:dev",
        "prebuild": "rimraf build && rimraf dist",
        "react:build": "react-scripts build",
        "react:dev": "cross-env BROWSER=none NODE_ENV=development react-scripts start",
        "electron:build": "electron-builder build --publish never",
        "electron:deploy": "electron-builder build --win --publish always",
        "electron:dev": "cross-env NODE_ENV=development electron .",
        "build": "npm run react:build",
        "deploy": "npm run react:build && npm run electron:deploy",
        "format": "prettier --write .",
        "update": "taze latest -w",
        "serve:chroma": "chroma run --path ${DB_PATH:-../chroma}",
        "executable:build": "cd ../executables && ./build.sh",
        "start": "cross-env NODE_ENV=production electron-forge start",
        "package": "cross-env NODE_ENV=production electron-forge package",
        "make": "npm run executable:build && npm run build && cross-env NODE_ENV=production electron-forge make"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
